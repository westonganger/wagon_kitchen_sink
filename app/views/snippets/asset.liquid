{% case type %}
{% when 'animate.css', 'animate-css', 'animate' %}
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.2/animate.min.css" />

{% when 'aos', 'animate-on-scroll' %}
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.css" />
  <script src="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.js"></script>

{% when 'bootstrap' %}
  {% assign version = version | default: '4.3.1' %}
  {% assign major_version = version | slice: 0 %}

  {% if bootswatch_theme && bootswatch_theme != '' %}
    {% assign the_path = 'https://stackpath.bootstrapcdn.com/bootswatch/the-version/the-theme/bootstrap.min.css' | replace_first: 'the-theme', bootswatch_theme %}

  {% else %}
    {% if major_version == '2' %}
      {% assign the_path = 'https://stackpath.bootstrapcdn.com/twitter-bootstrap/the-version/css/bootstrap-combined.min.css' %}
    {% else %}
      {% assign the_path = 'https://stackpath.bootstrapcdn.com/bootstrap/the-version/css/bootstrap.min.css' %}
    {% endif %}
  {% endif %}

  {% assign the_path = the_path | replace_first: 'the-version', version %}
  {{ the_path | stylesheet_tag }}

  {% if major_version == '2' %}
    {% assign the_path = 'https://stackpath.bootstrapcdn.com/twitter-bootstrap/the-version/js/bootstrap.min.js' %}
  {% elsif major_version == '3' %}
    {% assign the_path = 'https://stackpath.bootstrapcdn.com/bootstrap/the-version/js/bootstrap.min.js' %}
  {% else %}
    {% assign the_path = 'https://stackpath.bootstrapcdn.com/bootstrap/the-version/js/bootstrap.bundle.min.js' %}
  {% endif %}

  {% assign the_path = the_path | replace_first: 'the-version', version %}
  {{ the_path | javascript_tag }}

{% when 'bootstrap-datepicker' %}
  {% if bootstrap_version and (bootstrap_version == '3' or bootstrap_version == 3)
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker3.min.css" />
  {% else %}
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker.min.css" />
  {% endif %}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js"></script>

{% when 'bootstrap-switch' %}
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-switch/3.3.4/css/bootstrap3/bootstrap-switch.min.css" />
  <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-switch/3.3.4/js/bootstrap-switch.min.js"></script>

{% when 'bulma' %}
  {% assign version = version | default: '0.7.5' %}
  {% assign the_path = 'https://cdnjs.cloudflare.com/ajax/libs/bulma/the-version/css/bulma.min.css' | replace_first: 'the-version', version %}
  {{ the_path | stylesheet_tag }}

{% when 'flexslider', 'flex-slider' %}
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/flexslider/2.7.2/flexslider.min.css" />
  <script src="https://cdnjs.cloudflare.com/ajax/libs/flexslider/2.7.2/jquery.flexslider.min.js"></script>

{% when 'hover.css' %}
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/hover.css/2.3.1/css/hover-min.css" />

{% when 'hover-intent', 'jquery-hoverintent', 'jquery.hoverintent' %}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.hoverintent/1.10.0/jquery.hoverIntent.min.js"></script>

{% when 'isotope', 'jquery.isotope' %}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.isotope/3.0.6/isotope.pkgd.min.js"></script>

{% when 'jquery-easing', 'jquery.easing' %}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>

{% when 'jquery-magnific-popup', 'jquery.magnific-popup' %}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/jquery.magnific-popup.min.js"></script>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/magnific-popup.css" />

{% when 'jquery-visible' %}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-visible/1.2.0/jquery.visible.min.js"></script>

{% when 'jquery.sticky', 'jquery-sticky' %}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.sticky/1.0.4/jquery.sticky.min.js"></script>

{% when 'jquery-waypoints' %}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/waypoints/4.0.1/jquery.waypoints.min.js"></script>

{% when 'masonry' %}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/masonry/4.2.2/masonry.pkgd.min.js"></script>

{% when 'materialize' %}
  {% assign version = version | default: '1.0.0' %}

  {% assign the_path = 'https://cdnjs.cloudflare.com/ajax/libs/materialize/the-version/css/materialize.min.css' | replace_first: 'the-version', version %}
  {{ the_path | stylesheet_tag }}

  {% assign the_path = 'https://cdnjs.cloudflare.com/ajax/libs/materialize/the-version/js/materialize.min.js' | replace_first: 'the-version', version %}
  {{ the_path | javascript_tag }}

{% when 'milligram' %}
  {% assign version = version | default: '1.3.0' %}
  {% assign the_path = 'https://cdnjs.cloudflare.com/ajax/libs/milligram/the-version/milligram.min.css' | replace_first: 'the-version', version %}
  {{ the_path | stylesheet_tag }}

{% when 'no-ui-slider' %}
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/noUiSlider/13.1.5/nouislider.min.css" />
  <script src="https://cdnjs.cloudflare.com/ajax/libs/noUiSlider/13.1.5/nouislider.min.js"></script>

{% when 'owl-carousel' %}
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/owl-carousel/1.3.3/owl.carousel.min.css" />
  <script src="https://cdnjs.cloudflare.com/ajax/libs/owl-carousel/1.3.3/owl.carousel.min.js"></script>

{% when 'parallax' %}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/parallax/3.1.0/parallax.min.js"></script>

{% when 'parallax.js' %}
  {% comment %} Yes its different {% endcomment %}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/parallax.js/1.5.0/parallax.min.js"></script>

{% when 'semantic-ui' %}
  {% assign version = version | default: '2.4.2' %}

  {% assign the_path = 'https://cdn.jsdelivr.net/npm/semantic-ui@the-version/dist/semantic.min.css' | replace_first: 'the-version', version %}
  {{ the_path | stylesheet_tag }}

  {% assign the_path = 'https://cdn.jsdelivr.net/npm/semantic-ui@the-version/dist/semantic.min.js' | replace_first: 'the-version', version %}
  {{ the_path | javascript_tag }}

{% when 'stellar', 'stellar.js', 'jquery-stellar' %}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/stellar.js/0.6.2/jquery.stellar.min.js"></script>

{% when 'superfish' %}
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/superfish/1.7.10/css/superfish.min.css" />
  <script src="https://cdnjs.cloudflare.com/ajax/libs/superfish/1.7.10/js/superfish.min.js"></script>
  
{% when 'tether' %}
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.4/css/tether.min.css" />
  <script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.4/js/tether.min.js"></script>

{% when 'typed.js' %}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/typed.js/1.1.7/typed.min.js"></script>

{% when 'wow', 'wow.js' %}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/wow/1.1.2/wow.min.js"></script>

{% when '' %}
  {% comment %} dont bother warning when empty string is passed {% endcomment %}

{% else %}
  <script type="text/javascript">console.log("Invalid asset specified: {{ type }}");</script>

{% endcase %}
